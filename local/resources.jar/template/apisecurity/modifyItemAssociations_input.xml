    <!--Valid values of Action are Create and Modify. Set Action = "Create" to create new association. Set Action = "Modify" to modify association.-->
    <Association>
        <Item/>
        <Category/>
        <Query/>
    </Association>
    <!-- All matching Associations based on the input parameteres in Association element will be removed for the given Parent Item. -->
    <Remove>
        <Association>
            <Item/>
            <!-- Either ItemKey or ItemID, OrganizationCode, UnitOfMeasure is required. -->
            <Category/>
            <Query/>
        </Association>
    </Remove>
</AssociationList>
