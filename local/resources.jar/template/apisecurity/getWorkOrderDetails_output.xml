<WorkOrder>
    <OrderLine>
        <LinePriceInfo/>
        <ItemDetails>
            <PrimaryInformation/>
        </ItemDetails>
    </OrderLine>
    <Order>
        <PriceInfo/>
        <OverallTotals/>
    </Order>
    <Item>
        <PrimaryInformation/>
    </Item>
    <WorkOrderComponents>
        <WorkOrderComponent>
            <Item>
                <PrimaryInformation/>
            </Item>
            <WorkOrderComponentTag/>
        </WorkOrderComponent>
    </WorkOrderComponents>
    <WorkOrderActivities>
        <WorkOrderActivity>
            <WorkOrderActivityDtls>
                <WorkOrderActivityDtl/>
            </WorkOrderActivityDtls>
            <WorkOrderActivityInstructions>
                <Instruction/>
            </WorkOrderActivityInstructions>
            <AllowedModifications>
                <Modification/>
            </AllowedModifications>
        </WorkOrderActivity>
    </WorkOrderActivities>
    <WorkOrderStatusAudits>
        <WorkOrderStatusAudit>
            <OldStatus/>
            <NewStatus/>
            <Audits>
                <Audit>
                    <AuditDetail>
                        <IDs>
                            <ID/>
                        </IDs>
                        <Attributes>
                            <Attribute/>
                        </Attributes>
                    </AuditDetail>
                </Audit>
            </Audits>
        </WorkOrderStatusAudit>
    </WorkOrderStatusAudits>
    <PersonInfo/>
    <Status/>
    <WorkOrderServiceLines>
        <WorkOrderServiceLine>
            <OrderLine>
                <Shipnode>
                    <ShipNodePersonInfo/>
                </Shipnode>
                <Order/>
                <LinePriceInfo/>
                <ItemDetails>
                    <PrimaryInformation/>
                </ItemDetails>
            </OrderLine>
        </WorkOrderServiceLine>
    </WorkOrderServiceLines>
    <WorkOrderProdDeliveries>
        <WorkOrderProdDelivery>
            <OrderLine>
                <Order/>
                <LinePriceInfo/>
                <ItemDetails>
                    <PrimaryInformation/>
                </ItemDetails>
            </OrderLine>
        </WorkOrderProdDelivery>
    </WorkOrderProdDeliveries>
    <WorkOrderAppointments>
        <WorkOrderAppointment>
            <ResourcePoolCapacityConsumptionDetails/>
            <WorkOrderAppointmentUsers>
                <WorkOrderAppointmentUser/>
            </WorkOrderAppointmentUsers>
        </WorkOrderAppointment>
    </WorkOrderAppointments>
    <WorkOrderServiceTools>
        <WorkOrderServiceTool/>
    </WorkOrderServiceTools>
    <Instructions>
        <Instruction/>
    </Instructions>
    <AllowedModifications>
        <Modification/>
    </AllowedModifications>
    <!-- AllowedModifications is a template-driven element but its attributes and children elements are not. AllowedModifications element will be returned in the output only for non-history work orders. For history work orders no modifications allowed. If AllowedModifications is present in the template, then all it attributes and children elements will appear in the output.
   If a ModificationType appears in the output and ThroughOverride="Y" then that modification type can be called for the work order only in override mode.
   If a ModificationType appears in the output and ThroughOverride="N" then that modification type can be called for the work order either in non-override mode or called in override mode.
   -->
    <AnswerSets>
        <AnswerSet>
            <Answers>
                <Answer/>
            </Answers>
        </AnswerSet>
    </AnswerSets>
    <WorkOrderTag/>
    <WorkOrderHoldTypes>
        <WorkOrderHoldType>
            <WorkOrderHoldTypeLogs>
                <WorkOrderHoldTypeLog/>
            </WorkOrderHoldTypeLogs>
        </WorkOrderHoldType>
    </WorkOrderHoldTypes>
    <WorkOrderNotesList>
        <WorkOrderNotes/>
    </WorkOrderNotesList>
</WorkOrder>
