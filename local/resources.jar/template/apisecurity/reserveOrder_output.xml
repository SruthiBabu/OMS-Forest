<!--changeOrder Output XML-->
<Order>
    <OrderAudit>
        <Order/>
        <OrderAuditLevels>
            <OrderAuditLevel>
                <OrderLine>
                    <Item>
                        <LanguageDescriptionList>
                            <LanguageDescription/>
                        </LanguageDescriptionList>
                    </Item>
                </OrderLine>
                <ModificationTypes>
                    <ModificationType/>
                </ModificationTypes>
                <OrderRelease/>
                <OrderAuditDetails>
                    <OrderAuditDetail>
                        <IDs>
                            <!-- ID is not a template-driven element. If the template contains OrderAuditDetail element, this element will come based on AuditType. For AuditTypes like Order, OrderLine this element does not come.-->
                            <ID/>
                        </IDs>
                        <Attributes>
                            <!-- Attribute is not a template-driven element. If the template contains OrderAuditDetail element, this element will appear in the output xml alongwith all its attributes.-->
                            <Attribute/>
                        </Attributes>
                        <!-- CustomAuditDetails is not a template-driven element. If custom entity is modified, this element will appear in the output xml . -->
                        <!-- Nested CustomAuditDetails element will be returned if custom table has a hangoff table. -->
                        <CustomAuditDetails>
                            <OrderAuditDetails>
                                <OrderAuditDetail>
                                    <IDs>
                                    <!-- ID is not a template-driven element. If the template contains OrderAuditDetail element, this element will come based on AuditType. For AuditTypes like Order, OrderLine this element does not come.-->
                                    <ID/>
                                    </IDs>
                                    <Attributes>
                                    <!-- Attribute is not a template-driven element. If the template contains OrderAuditDetail element, this element will appear in the output xml alongwith all its attributes.-->
                                    <Attribute/>
                                    </Attributes>
                                </OrderAuditDetail>
                            </OrderAuditDetails>
                        </CustomAuditDetails>
                    </OrderAuditDetail>
                </OrderAuditDetails>
            </OrderAuditLevel>
        </OrderAuditLevels>
    </OrderAudit>
    <UnprocessedLines>
        <!-- The actual output xml will contain the Orderline element, its sub-elements and their attributes in exact same manner as were passed in the input xml.   -->
        <OrderLine/>
    </UnprocessedLines>
    <PriceInfo/>
    <OrderLines>
        <!--1 or more order
        line-->
        <OrderLine>
            <Item>
                <LanguageDescriptionList>
                    <LanguageDescription/>
                </LanguageDescriptionList>
            </Item>
            <!--Optional-->
