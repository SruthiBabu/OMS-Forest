                <LineTax/>
                <TaxSummary>
                    <TaxSummaryDetail/>
                </TaxSummary>
            </LineTaxes>
            <!-- LineTax is not a template driven element. If the template contains either of LineCharge or LineTax elements, both will appear in the output, alongwith all their attributes. -->
            <!--Optional-->
            <OrderStatuses>
                <OrderStatus>
                    <Details>
                        <KitDetails>
                            <KitDetail>
                                <KitDetailTranQuantity/>
                            </KitDetail>
                        </KitDetails>
                    </Details>
                    <OrderStatusTranQuantity/>
                </OrderStatus>
            </OrderStatuses>
            <OrderLineTranQuantity/>
            <DerivedFromOrderLine>
                <Item/>
            </DerivedFromOrderLine>
            <BundleComponents>
                <BundleComponent>
                    <Item/>
                </BundleComponent>
            </BundleComponents>
            <OrderHoldTypes>
                <OrderHoldType>
                    <OrderHoldTypeLogs>
                        <OrderHoldTypeLog/>
                    </OrderHoldTypeLogs>
                </OrderHoldType>
            </OrderHoldTypes>
        </OrderLine>
