<!--MasterOrder XML-->
<Order>
    <OrderLines>
        <!--1 or more order
        line-->
        <OrderLine>
            <Item>
                <LanguageDescriptionList>
                    <LanguageDescription/>
                </LanguageDescriptionList>
            </Item>
            <WorkOrders>
                <WorkOrder/>
            </WorkOrders>
            <OrderLineTranQuantity/>
            <OrderLineOptions>
                <OrderLineOption/>
            </OrderLineOptions>
            <OrderLineSourcingControls>
                <OrderLineSourcingCntrl/>
            </OrderLineSourcingControls>
            <DerivedFromOrder/>
            <PersonInfoShipTo/>
            <!--Optional-->
            <PersonInfoMarkFor/>
            <!--Optional-->
            <LinePriceInfo/>
            <Instructions>
                <!-- NumberOfInstructions is not a template driven attribute. -->
                <Instruction/>
            </Instructions>
            <References>
                <Reference/>
            </References>
            <!--Optional-->
            <KitLines>
                <!-- NumberOfKitLines is not a template driven attribute. -->
                <KitLine>
                    <KitLineTranQuantity/>
                </KitLine>
            </KitLines>
            <Schedules>
                <Schedule>
                    <ScheduleTranQuantity/>
                </Schedule>
            </Schedules>
            <AdditionalAddresses>
                <!-- NumberOfAdditionalAddresses is not a template driven attribute. -->
                <AdditionalAddress>
                    <PersonInfo/>
                </AdditionalAddress>
            </AdditionalAddresses>
            <LineOverallTotals/>
            <!-- LineOverallTotals is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if the attributes are removed from the template, they will still be present in the output xml. -->
            <LineInvoicedTotals/>
            <!-- LineInvoicedTotals is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if the attributes are removed from the template, they will still be present in the output xml. -->
            <LineRemainingTotals/>
            <!-- LineRemainingTotals is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if the attributes are removed from the template, they will still be present in the output xml. -->
            <LineOverallTotalsInEnterpriseCurrency/>
            <!-- LineOverallTotalsInEnterpriseCurrency is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if only the attributes are removed from the template, they will still be present in the output xml. -->
            <LineRemainingTotalsInEnterpriseCurrency/>
            <!-- LineRemainingTotalsInEnterpriseCurrency is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if only the attributes are removed from the template, they will still be present in the output xml. -->
            <LineCharges>
                <LineCharge/>
            </LineCharges>
            <!-- LineCharge is not a template driven element. If the template contains either of LineCharge or LineTax elements, both will appear in the output, alongwith all their attributes. -->
            <LineTaxes>
                <LineTax/>
                <TaxSummary>
                    <TaxSummaryDetail/>
                </TaxSummary>
            </LineTaxes>
            <!-- LineTax is not a template driven element. If the template contains either of LineCharge or LineTax elements, both will appear in the output, alongwith all their attributes. -->
            <!--Optional-->
            <OrderStatuses>
                <OrderStatus>
                    <OrderStatusTranQuantity/>
                    <Details>
                        <KitDetails>
                            <KitDetail>
                                <KitDetailTranQuantity/>
                            </KitDetail>
                        </KitDetails>
                    </Details>
                </OrderStatus>
            </OrderStatuses>
            <OrderDates>
                <OrderDate/>
            </OrderDates>
            <OrderLineInvAttRequest/>
            <ProductAssociations>
                <ProductAssociation>
                    <ProductLine/>
                    <ServiceLine/>
                </ProductAssociation>
            </ProductAssociations>
            <ServiceAssociations>
                <ServiceAssociation>
                    <ProductLine/>
                    <ServiceLine/>
                </ServiceAssociation>
            </ServiceAssociations>
            <DeliveryAssociations>
                <DeliveryAssociation>
                    <ProductLine/>
                    <ServiceLine/>
                </DeliveryAssociation>
            </DeliveryAssociations>
            <ChildOrderLineRelationships>
                <OrderLineRelationship>
                    <ParentLine/>
                    <ChildLine/>
                </OrderLineRelationship>
            </ChildOrderLineRelationships>
            <ParentOrderLineRelationships>
                <OrderLineRelationship>
                    <ParentLine/>
                    <ChildLine/>
                </OrderLineRelationship>
            </ParentOrderLineRelationships>
            <DerivedFromOrderLine>
                <Item/>
            </DerivedFromOrderLine>
            <Notes>
                <Note/>
            </Notes>
            <BundleParentLine/>
            <CustomAttributes/>
        </OrderLine>
    </OrderLines>
    <Notes>
        <Note/>
    </Notes>
    <Instructions>
        <!-- NumberOfInstructions is not a template driven attribute. -->
        <Instruction/>
    </Instructions>
    <OrderTags>
        <OrderTag/>
    </OrderTags>
    <!--Optional-->
    <PersonInfoShipTo/>
    <!--Either at  line level or at header
    level-->
    <PersonInfoBillTo/>
    <PersonInfoMarkFor/>
    <PersonInfoContact/>
    <AdditionalAddresses>
        <AdditionalAddress>
            <PersonInfo/>
        </AdditionalAddress>
    </AdditionalAddresses>
    <PriceInfo/>
    <PriceInfoInEnterpriseCurrency/>
    <OverallTotals/>
    <!-- OverallTotals is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if the attributes are removed from the template, they will still be present in the output xml. -->
    <InvoicedTotals/>
    <!-- InvoicedTotals is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if the attributes are removed from the template, they will still be present in the output xml. -->
    <RemainingTotals/>
    <!-- RemainingTotals is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if the attributes are removed from the template, they will still be present in the output xml. -->
    <OverallTotalsInEnterpriseCurrency/>
    <!-- OverallTotalsInEnterpriseCurrency is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if only the attributes are removed from the template, they will still be present in the output xml. -->
    <RemainingTotalsInEnterpriseCurrency/>
    <!-- RemainingTotalsInEnterpriseCurrency is a template-driven element but its attributes are not. The element can be removed from the template, and prevented from appearing in the output. But if only the attributes are removed from the template, they will still be present in the output xml. -->
    <HeaderCharges>
        <HeaderCharge/>
    </HeaderCharges>
    <!-- HeaderCharge is not a template driven element. If the template contains either of HeaderCharge or HeaderTax elements, both will appear in the output, alongwith all their attributes. -->
    <HeaderTaxes>
        <HeaderTax/>
        <TaxSummary>
            <TaxSummaryDetail/>
        </TaxSummary>
    </HeaderTaxes>
    <!-- HeaderTax is not a template driven element. If the template contains either of HeaderCharge or HeaderTax elements, both will appear in the output, alongwith all their attributes. -->
    <References>
        <Reference/>
    </References>
    <!--Optional-->
    <PaymentMethods>
        <PaymentMethod>
            <PersonInfoBillTo/>
        </PaymentMethod>
    </PaymentMethods>
    <RemainingFinancialTotals/>
    <ChargeTransactionDetails>
        <!-- The attributes of ChargeTransactionDetails element are not template driven. If the element is present in the template, all the attributes will be output, regardless of their presence or absence in the template. -->
        <ChargeTransactionDetail>
            <CreditCardTransactions>
                <CreditCardTransaction/>
            </CreditCardTransactions>
            <PaymentMethod/>
        </ChargeTransactionDetail>
    </ChargeTransactionDetails>
    <OrderStatuses>
        <OrderStatus>
            <OrderStatusTranQuantity/>
            <Details>
                <KitDetails>
                    <KitDetail>
                        <KitDetailTranQuantity/>
                    </KitDetail>
                </KitDetails>
            </Details>
        </OrderStatus>
    </OrderStatuses>
    <OrderDates>
        <OrderDate/>
    </OrderDates>
    <ProductServiceAssocs>
        <ProductServiceAssoc>
            <ProductLine/>
            <ServiceLine/>
        </ProductServiceAssoc>
    </ProductServiceAssocs>
    <OrderLineRelationships>
        <OrderLineRelationship>
            <ParentLine/>
            <ChildLine/>
        </OrderLineRelationship>
    </OrderLineRelationships>
    <SpecialServices>
        <SpecialService/>
    </SpecialServices>
    <PersonInfoSoldTo/>
    <Opportunity>
        <BuyerContactAddress/>
        <Notes>
            <Note/>
        </Notes>
        <CustomAttributes/>
    </Opportunity>
    <FileAttachments>
        <!--1 or more attachments-->
        <FileAttachment/>
    </FileAttachments>
    <CustomAttributes/>
    <OrderSearchConditions>
        <OrderSearchCondition/>
    </OrderSearchConditions>
</Order>
