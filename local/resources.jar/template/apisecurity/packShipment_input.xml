<!-- Either ShipmentKey or ShipmentNo is required -->
<!-- OverrideModificationRules is used to override the statuses of shipment that can be considered for pack. This is determined using the modification_type SHIPMENT_PACK -->
<Shipment>
    <Containers>
        <!-- Only one Container will be processed in one call to this API. All other Container nodes will be ignored. Read Javadocs for additional details. -->
        <Container>
            <ContainerDetails>
                <!--One amongst ShipmentLineKey OR ShipmentLine child element must be passed-->
                <ContainerDetail>
                    <ShipmentLine/>
                    <ShipmentTagSerials>
                        <ShipmentTagSerial>
                            <SerialRange/>
                        </ShipmentTagSerial>
                    </ShipmentTagSerials>
                </ContainerDetail>
            </ContainerDetails>
            <Instructions>
                <Instruction/>
            </Instructions>
            <AdditionalAttributes>
                <AdditionalAttribute/>
            </AdditionalAttributes>
            <SpecialServices>
                <SpecialService/>
            </SpecialServices>
        </Container>
    </Containers>
</Shipment>
