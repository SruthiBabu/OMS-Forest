<DBSchema>
    <Entities>
   		<Entity AuditRequired="Y" Cacheable="true" Description="This table stores Change Project definitions. 
   		A Change Project provides the mechanism to consolidate database changes under a single unit. 
   		A Change Project can be approved on one system so that the database changes associated with it can be sent to another system.
		A Change Project is a collection of Change Requests. It is also the unit which a valid user can approve or reject. 
		All data changed as part of a Change Project will be logged under the same Change Project."
            Module="ysc" Name="Change_Project" Prefix="YFS_" ConfigurableByOrgOrNode="Y"
            TableName="YFS_CHANGE_PROJECT" EntityType="CONFIGURATION" TableType="CONFIGURATION" XMLName="ChangeProject">
            <Attributes>
                <Attribute ColumnName="CHANGE_PROJECT_KEY"
                    DataType="Key" DefaultValue="' '"
                    Description="Primary key attribute of YFS_CHANGE_PROJECT."
                    Name="Change_Project_Key" Nullable="false" XMLName="ChangeProjectKey"/>
                <Attribute ColumnName="ORGANIZATION_CODE"
                    DataType="OrgCode" IsOrgColumn="Y" DataSecurityUsageType="ORG"
                    Description="The organization that defines the Change Project."
                    Name="Organization_Code" Nullable="true" XMLName="OrganizationCode"/>
                <Attribute ColumnName="CHANGE_PROJECT_ID"
                    DataType="VarcharID-24" Description="The identifier for the Change Project. If it is not passed during creation, 
                    it is generated and pre-fixed with the Deployment Type prefix. It cannot be set to DEFAULT."
                    Name="Change_Project_ID" Nullable="true" XMLName="ChangeProjectID"/>
                <Attribute ColumnName="WORKSPACE_ID"
                    DataType="VarcharID-24" DefaultValue="DEFAULT"
                    Description="The identifier for the workspace to which the Change Project belongs."
                    Name="Workspace_ID" Nullable="true" XMLName="WorkspaceID"/>
                <Attribute ColumnName="USER_GROUP_ID"
                    DataType="UserGroup"  
                    Description="The identifier for the user group that can approve the Change Project."
                    Name="User_Group_ID" Nullable="true" XMLName="UserGroupID"/>
                <Attribute ColumnName="TEAM_CODE"
                    DataType="DataSecurityGroupId" 
                    Description="The identifier for the team that can approve the Change Project."
                    Name="Team_Code" Nullable="true" XMLName="TeamCode"/>
                <Attribute ColumnName="USE_ENTERPRISE_SYNC_TIME"
                    DataType="Flag" 
                    Description="If set as Y, indicates that sync time needs to be determined from the enterprise configuration."
                    Name="Use_Enterprise_Sync_Time" Nullable="true" XMLName="UseEnterpriseSyncTime"/>
                <Attribute ColumnName="MANAGE_USER_ID"
                    DataType="LoginId-50" 
                    Description="Identifier for the user who caused a change in the Change Project. This is always taken from the Environment."
                    Name="Manage_User_ID" Nullable="true" XMLName="ManageUserID"/>
                <Attribute ColumnName="STATUS"
                    DataType="ChangeProjectStatus" 
                    Description="The status of the Change Project. As the Change Project progresses through the change process, the status changes."
                    Name="Status" Nullable="true" XMLName="Status"/>
                <Attribute ColumnName="IS_IMPORTED"
                    DataType="Flag" 
                    Description="If set as Y, indicates whether this Change Project was imported from another instance."
                    Name="Is_Imported" Nullable="true" XMLName="IsImported"/>
                <Attribute ColumnName="SHORT_DESCRIPTION"
                    DataType="Text-100" 
                    Description="The short description of the Change Project."
                    Name="Short_Description" Nullable="true" XMLName="ShortDescription"
                    Localized="true"/>
                <Attribute ColumnName="LONG_DESCRIPTION"
                    DataType="Description-255" 
                    Description="The long description of the Change Project."
                    Name="Long_Description" Nullable="true" XMLName="LongDescription"
                    Localized="true"/>
                 <Attribute ColumnName="CREATETS" DataType="TimeStamp"
                    DefaultValue="sysdate"
                    Description="The timestamp for the creation of this record."
                    Name="Createts" Nullable="false" XMLName="Createts"/>
                <Attribute ColumnName="MODIFYTS" DataType="TimeStamp"
                    DefaultValue="sysdate "
                    Description="The timestamp for the last modification of this record."
                    Name="Modifyts" Nullable="false" XMLName="Modifyts"/>
                <Attribute ColumnName="CREATEUSERID"
                    DataType="Createuserid"
                    Description="The user who created this record."
                    Name="Createuserid" Nullable="true" XMLName="Createuserid"/>
                <Attribute ColumnName="MODIFYUSERID"
                    DataType="Modifyuserid"
                    Description="The user who last modified this record."
                    Name="Modifyuserid" Nullable="true" XMLName="Modifyuserid"/>
                <Attribute ColumnName="CREATEPROGID"
