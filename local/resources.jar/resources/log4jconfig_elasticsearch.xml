<log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/">
	<category name="org.elasticsearch" class="com.yantra.yfc.log.YFCLogCategory" additivity="false" >
        <level class="com.yantra.yfc.log.YFCLogLevel" value="VERBOSE"  />
        <appender-ref ref="CONSOLE" />
        <appender-ref ref="ALL" />
    </category>
    <category name="netty.channel" class="com.yantra.yfc.log.YFCLogCategory" additivity="false" >
        <level class="com.yantra.yfc.log.YFCLogLevel" value="VERBOSE"  />
        <appender-ref ref="CONSOLE" />
        <appender-ref ref="ALL" />
    </category>
    <category name="io.searchbox.client" class="com.yantra.yfc.log.YFCLogCategory" additivity="false" >
        <level class="com.yantra.yfc.log.YFCLogLevel" value="WARN"  />
        <appender-ref ref="CONSOLE" />
        <appender-ref ref="ALL" />
    </category>
</log4j:configuration>
